{
    "name": "mpociot/captainhook",
    "license": "MIT",
    "description": "Add webhooks to your Laravel app.",
    "keywords": [
        "events",
        "hook",
        "laravel",
        "webhook",
        "webhooks"
    ],
    "homepage": "http://github.com/mpociot/captainhook",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mpociot/captainhook/issues",
        "source": "https://github.com/mpociot/captainhook"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/bus": "^5.8|^6.0|^7.0",
        "illuminate/queue": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "illuminate/database": "^5.8|^6.0|^7.0",
        "illuminate/events": "^5.8|^6.0|^7.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.8|^4.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-0": {
            "Mpociot\\CaptainHook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpociot\\CaptainHook\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpociot\\CaptainHook\\CaptainHookServiceProvider"
            ]
        }
    }
}
